.tool-bar-default {
    width: 100%;
    height: 1%;
    background-color: rgb(233, 233, 233);
    min-width: 100%;
    min-height: 20%;
    max-height: 30%;
}

.work-space-default {
    width: 95%;
    align-self: center;
    height: 50%;
}

.tool-bar-button {
    width: 50px;
    height: 90px;
    align-self: auto;
    justify-content: flex-start;
    align-items: auto;
    flex-direction: column;
    flex-wrap: nowrap;
    -unity-text-align: upper-center;
    white-space: nowrap;
    background-color: rgba(255, 255, 255, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    background-image: url("project://database/Assets/Images/Editor/file-add.png?fileID=2800000&guid=32c7d6969f5e24544905de4c13905b71&type=3#file-add");
}

.tool-bar-button-text {
    align-self: center;
    justify-content: flex-end;
    align-items: center;
    top: -20px;
    text-overflow: clip;
    white-space: normal;
    flex-wrap: nowrap;
}
